					<i class="bi bi-person"></i>
					<span>Home</span>
				</a>
			</li><!-- End Profile Page Nav -->

			<li class="nav-item">
				<a class="nav-link collapsed" asp-controller="ToDoList" asp-action="TaskList">
					<i class="bi bi-receipt" style="font-size:x-large"></i>
					<span>Task List</span>
				</a>
			</li>

			<li class="nav-item">
				<a class="nav-link collapsed" asp-controller="SubTask" asp-action="SubTaskList">
					<i class="bi bi-receipt" style="font-size:x-large"></i>
